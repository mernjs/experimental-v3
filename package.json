{
  "name": "project_name",
  "version": "0.1.0",
  "main": "electron.js",
  "private": true,
  "scripts": {
    "web:start": "react-scripts start",
    "web:build": "react-scripts build",
    "desktop:build": "electron-builder",
    "build": "npm run web:build && npm run desktop:build -wml",
    "dev": "concurrently \"cross-env BROWSER=none npm run web:start\" \"wait-on http://localhost:3000 && nodemon --exec electron .\""
  },
  "author": "author_email",
  "homepage": "./",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^26.0.23",
    "@types/node": "^12.20.15",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@types/redux-logger": "^3.0.8",
    "cross-env": "^7.0.3",
    "electron-is-dev": "^2.0.0",
    "history": "^5.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "redux": "^4.0.5",
    "redux-form": "^8.3.7",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "typescript": "^4.3.2"
  },
  "devDependencies": {
    "@types/redux-form": "^8.3.1",
    "concurrently": "^6.2.0",
    "electron": "^13.1.7",
    "electron-builder": "^22.11.7",
    "nodemon": "^2.0.12",
    "wait-on": "^6.0.0"
  },
  "build": {
    "appId": "react.electron",
    "productName": "react-electron",
    "mac": {
      "icon": "build/logo512.png",
      "target": "tar.gz"
    },
    "linux": {
      "category": "Utils",
      "icon": "build/icon.png",
      "packageCategory": "utils",
      "maintainer": "author_email",
      "asar": true,
      "target": [
        "deb",
        "tar.xz"
      ],
      "synopsis": "Basically same as description but shorter"
    },
    "win": {
      "icon": "build/icon.png",
      "target": "nsis"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
